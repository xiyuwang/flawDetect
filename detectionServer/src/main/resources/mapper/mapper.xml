            </if>
        </where>
    </select>

    <select id="cameraExist" parameterType="com.detect.domain.CameraDO"
            resultType="boolean">
        SELECT EXISTS(SELECT camid FROM tb_cam ca
        <where>
            1 = 1
            <if test="id != null and id != '' ">
                and ca.id=#{id}
